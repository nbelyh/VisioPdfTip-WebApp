{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "tests",
      "type": "dotnet",
      "request": "launch",
      "projectPath": "${workspaceFolder}/tests/tests.csproj"
    },
    {
      "command": "./node_modules/.bin/astro dev",
      "name": "Development server",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "name": "Attach to .NET Functions",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:azureFunctions.pickProcess}"
    }
  ]
}
